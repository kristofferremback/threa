{
  "name": "threa",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun dev:deps && concurrently \"bun dev:server\" \"bun dev:client\"",
    "dev:deps": "docker compose up -d",
    "dev:server": "bun --watch src/server/index.ts",
    "dev:client": "vite",
    "build": "bun build:client",
    "build:client": "vite build",
    "start": "NODE_ENV=production bun src/server/index.ts",
    "preview": "vite preview",
    "stop:deps": "docker compose down",
    "clear:deps": "docker compose down -v",
    "langfuse": "docker compose -f docker-compose.langfuse.yml up -d",
    "langfuse:stop": "docker compose -f docker-compose.langfuse.yml down",
    "langfuse:clear": "docker compose -f docker-compose.langfuse.yml down -v",
    "test": "TEST_DATABASE_URL=postgresql://threa:threa@localhost:5434/threa_test bun test src/server --max-concurrency=1",
    "test:unit": "TEST_DATABASE_URL=postgresql://threa:threa@localhost:5434/threa_test bun test src/server/services",
    "test:e2e": "TEST_DATABASE_URL=postgresql://threa:threa@localhost:5434/threa_test bun test src/server/__tests__/e2e --max-concurrency=1",
    "test:evolution": "TEST_DATABASE_URL=postgresql://threa:threa@localhost:5434/threa_test bun test src/server/services/memo-evolution/__tests__",
    "test:deps": "docker compose -f docker-compose.test.yml up -d",
    "eval:evolution": "bun src/server/evals/cli.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.0",
